{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { required, minLength } from 'vuelidate/lib/validators';\nimport customValidator from '@/helper/validator';\nexport default {\n  data() {\n    return {\n      user: {\n        telephone: '',\n        password: ''\n      },\n      validation: null\n    };\n  },\n\n  validations: {\n    user: {\n      telephone: {\n        required,\n        telephone: customValidator.telephoneValidator\n      },\n      password: {\n        required,\n        minLength: minLength(6)\n      }\n    }\n  },\n  methods: {\n    validationState(name) {\n      // 这里是es6 机构赋值\n      const {\n        $dirty,\n        $error\n      } = this.$v.user[name];\n      return $dirty ? !$error : null;\n    },\n\n    login() {\n      console.log(\"login\");\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsCA;AACA;AACA;EACAA;IACA;MACAC;QACAC,aADA;QAEAC;MAFA,CADA;MAKAC;IALA;EAOA,CATA;;EAUAC;IACAJ;MACAC;QACAI,QADA;QAEAJ;MAFA,CADA;MAKAC;QACAG,QADA;QAEAC;MAFA;IALA;EADA,CAVA;EAuBAC;IACAC;MACA;MACA;QAAAC;QAAAC;MAAA;MACA;IACA,CALA;;IAMAC;MACAC;IACA;;EARA;AAvBA","names":["data","user","telephone","password","validation","validations","required","minLength","methods","validationState","$dirty","$error","login","console"],"sourceRoot":"src/views/login","sources":["Login.vue"],"sourcesContent":["<template>\n  <div class=\"register\">\n    <b-row class=\"mt-5\">\n      <b-col md=\"8\" offset-md=\"2\" lg=\"6\" offset-lg=\"3\">\n    <b-card title=\"登录\">\n    <b-form>\n      <b-form-group label=\"手机号:\">\n        <b-form-input\n          v-model=\"$v.user.telephone.$model\"\n          type=\"number\"\n          placeholder=\"输入手机号\"\n          :state=\"validationState('telephone')\"\n        ></b-form-input>\n        <b-form-invalid-feedback :state=\"validationState('telephone')\">\n          手机号不符合要求\n        </b-form-invalid-feedback>\n      </b-form-group>\n      <b-form-group label=\"密码:\">\n        <b-form-input\n          v-model=\"$v.user.password.$model\"\n          type=\"password\"\n          placeholder=\"输入密码\"\n          :state=\"validationState('password')\"\n        ></b-form-input>\n        <b-form-invalid-feedback :state=\"validationState('password')\">\n          密码必须大于6位\n        </b-form-invalid-feedback>\n      </b-form-group>\n      <b-form-group>\n        <b-button @click=\"login\" variant=\"outline-primary\" block>登录</b-button>\n      </b-form-group>\n      </b-form>\n      </b-card>\n        </b-col>\n      </b-row>\n  </div>\n</template>\n<script>\nimport { required, minLength } from 'vuelidate/lib/validators';\nimport customValidator from '@/helper/validator';\nexport default {\n  data() {\n    return {\n      user: {\n        telephone: '',\n        password: '',\n      },\n      validation: null,\n    };\n  },\n  validations: {\n    user: {\n      telephone: {\n        required,\n        telephone: customValidator.telephoneValidator,\n      },\n      password: {\n        required,\n        minLength: minLength(6),\n      },\n    },\n  },\n\n  methods: {\n    validationState(name) {\n      // 这里是es6 机构赋值\n      const {$dirty, $error} = this.$v.user[name];\n      return $dirty ? !$error : null;\n    }, \n    login() {\n      console.log(\"login\");\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}